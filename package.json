    "redux-auth": "https://bitbucket.org/arabellatech/redux-auth.git#4.0.2",
    "redux-connect-backend": "https://bitbucket.org/arabellatech/redux-connect-backend.git#0.3.0",
    "redux-form": "^7.0.1",
    "redux-form-stripe-fields": "https://bitbucket.org/arabellatech/redux-form-stripe-fields#1.0.1",
    "redux-immutable": "4.0.0",
    "redux-modal": "https://bitbucket.org/arabellatech/redux-modal#1.1.3",
    "redux-saga": "^0.15.6",
    "redux-saga-test-plan": "^3.2.0",
    "reselect": "3.0.1",
    "sanitize.css": "5.0.0",
    "styled-components": "^2.1.2",
    "survey-form": "https://bitbucket.org/arabellatech/survey-form",
    "warning": "3.0.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
