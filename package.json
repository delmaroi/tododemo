    "immutable": "3.8.1",
    "intl": "1.2.5",
    "invariant": "2.2.2",
    "ip": "1.1.5",
    "lodash": "4.17.4",
    "minimist": "1.2.0",
    "moment": "^2.18.1",
    "object-diff": "^0.0.4",
    "react": "^16.0.0",
    "react-avatar-management": "https://bitbucket.org/arabellatech/react-avatar-management#1.0.1",
    "react-chat": "https://bitbucket.org/arabellatech/react-chat#1.23.0",
    "react-dom": "^16.0.0",
    "react-form-fields": "https://bitbucket.org/arabellatech/react-form-fields#1.3",
    "react-helmet": "^5.2.0",
    "react-infinite": "^0.12.1",
    "react-intl": "^2.4.0",
    "react-mentions": "^1.2.1",
    "react-places-autocomplete": "^5.4.3",
    "react-redux": "^5.0.6",
    "react-resize-detector": "^0.6.0",
    "react-router": "^4.2.0",
